# .env.example - Copy this to .env and fill in your values
# API keys and configuration settings

# Proxy configuration (optional - set USE_PROXY=false to disable)
USE_PROXY=false
PROXY_TYPE=free  # free, scraperapi, smartproxy, aws
PROXY_API_KEY=your_proxy_api_key_here

# Cookie configuration (required - see COOKIES.md for setup instructions)
COOKIE_COUNT=0
# Add your cookies here in the format:
# COOKIE_COOKIENAME=value
# Example:
# COOKIE_SSLB=1
# COOKIE_PLUS_COOKIE_LEVEL=3

# Request configuration
REQUEST_TIMEOUT=30
MAX_RETRIES=3
BACKOFF_FACTOR=2

# Scraping configuration
MAX_CONCURRENT_REQUESTS=5
REQUEST_DELAY=0.5

# PLUS specific settings (required - obtain from browser dev tools)
PLUS_CSRF_TOKEN=your_csrf_token_here
